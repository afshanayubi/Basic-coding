#include <iostream>
using namespace std;
int main() {
    int n,i,j;
    cin>>n;
    if(n==2){
        cout<<"2"<<endl;
    }
    else{
        cout<<2<<endl;
    }
    
    for(i=3;i<=n;i++){
        int no=i;
        for( j=2;j<=(no-1);j++){
            if((no%j)==0)
            break;
            cout<<no<<endl;
        }
    }
    return 0;
}
